{
  "name": "react-chrome-extension",
  "version": "0.2.0.48",
  "description": "react chrome extension template",
  "scripts": {
    "clean": "rm -rf dist/* packages/*",
    "start": "webpack --watch --progress --mode development",
    "build": "webpack --progress --mode production",
    "package:chrome": "rm -f packages/extension.chrome*.zip; cd dist/chrome; zip -r ../../packages/extension.chrome.$npm_package_version.zip .",
    "package:edge": "rm -f packages/extension.edge*zip; cd dist/edge; zip -r ../../packages/extension.edge.$npm_package_version.zip .",
    "package:firefox": "rm -f packages/extension.firefox*.zip; web-ext build",
    "package": "npm run package:chrome; npm run package:firefox; npm run package:edge",
    "publish": "./scripts/build_env.sh; npx web-ext-deploy --env --verbose",
    "deploy:edge": "ts-node --esm scripts/deploy_edge.ts"
  },
  "author": "",
  "devDependencies": {
    "@mui/material": "^5.0.0",
    "@mui/styled-engine-sc": "^5.0.0",
    "@types/chrome": "^0.0.134",
    "@types/lodash": "^4.14.186",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/styled-components": "^5.1.14",
    "@types/webextension-polyfill": "^0.8.0",
    "chrome-webstore-upload-cli": "^2.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.2.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^4.5.2",
    "ifdef-loader": "^2.3.2",
    "install": "^0.13.0",
    "npm": "^8.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "shelljs": "^0.8.5",
    "style-loader": "^2.0.0",
    "styled-components": "^5.3.1",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.1.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.2.4",
    "web-ext": "^7.2.0",
    "web-ext-deploy": "^0.8.4",
    "webextension-polyfill": "^0.8.0",
    "webpack": "^5.34.0",
    "webpack-cli": "^4.6.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@sentry/integrations": "^7.19.0",
    "@sentry/node": "^7.19.0",
    "@sentry/react": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "@sentry/types": "^7.19.0",
    "lodash": "^4.17.21",
    "mutation-summary": "^1.0.0",
    "react-bootstrap": "^2.8.0",
    "react-icons": "^4.6.0",
    "ts-node": "^10.9.1"
  }
}
